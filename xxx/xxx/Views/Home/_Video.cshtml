@model xxx.Videos



<div class="modal-dialog">
    <div class="modal-content" style="background-color:black;">
        <div class="modal-header">
            <button type="button" style="color: white;font-size: 30px;margin-top:6px;" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            <h2 class="modal-title" style="color: white; text-align: center;">@Model.Title</h2>
        </div>
        <div class="modal-body">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 text-center">
                <iframe id="video" class="foo videoH"  frameborder="0" scrolling="no" src="@Model.Url" allowfullscreen></iframe>
            </div>
        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-default btn-primary" data-dismiss="modal" >Close</button>

        </div>
    </div><!-- /.modal-content -->
</div><!-- /.modal-dialog -->
